<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.group3.glimpse.SearchActivity">

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="368dp"
        android:layout_height="543dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <MultiAutoCompleteTextView
            android:id="@+id/searchBox"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Search" />

        <TextView
            android:id="@+id/textView_0"
            android:layout_width="match_parent"
            android:layout_height="31dp"
            android:text=""
            android:textAppearance="@android:style/TextAppearance.Material.SearchResult.Title"/>

        <TextView
            android:id="@+id/textView_1"
            android:layout_width="match_parent"
            android:layout_height="31dp"
            android:text=""
            android:textAppearance="@android:style/TextAppearance.Material.SearchResult.Title"/>

        <TextView
            android:id="@+id/textView_2"
            android:layout_width="match_parent"
            android:layout_height="31dp"
            android:text=""
            android:textAppearance="@android:style/TextAppearance.Material.SearchResult.Title"/>

        <TextView
            android:id="@+id/textView_3"
            android:layout_width="match_parent"
            android:layout_height="31dp"
            android:text=""
            android:textAppearance="@android:style/TextAppearance.Material.SearchResult.Title"/>

        <TextView
            android:id="@+id/textView_4"
            android:layout_width="match_parent"
            android:layout_height="31dp"
            android:text=""
            android:textAppearance="@android:style/TextAppearance.Material.SearchResult.Title"/>

        <TextView
            android:id="@+id/textView_5"
            android:layout_width="match_parent"
            android:layout_height="31dp"
            android:textAppearance="@android:style/TextAppearance.Material.SearchResult.Title" />

        <TextView
            android:id="@+id/textView_6"
            android:layout_width="match_parent"
            android:layout_height="31dp"
            android:text=""
            android:textAppearance="@android:style/TextAppearance.Material.SearchResult.Title"/>

        <TextView
            android:id="@+id/textView_7"
            android:layout_width="match_parent"
            android:layout_height="31dp"
            android:text=""
            android:textAppearance="@android:style/TextAppearance.Material.SearchResult.Title"/>

        <TextView
            android:id="@+id/textView_8"
            android:layout_width="match_parent"
            android:layout_height="31dp"
            android:text=""
            android:textAppearance="@android:style/TextAppearance.Material.SearchResult.Title"/>

        <TextView
            android:id="@+id/textView_9"
            android:layout_width="match_parent"
            android:layout_height="31dp"
            android:text=""
            android:textAppearance="@android:style/TextAppearance.Material.SearchResult.Title"/>

        <Button
            android:id="@+id/searchBtn"
            android:layout_width="358dp"
            android:layout_height="wrap_content"
            android:backgroundTint="@color/com_facebook_button_send_background_color_pressed"
            android:text="Search" />

        <Button
            android:id="@+id/back"
            android:layout_width="357dp"
            android:layout_height="wrap_content"
            android:backgroundTint="@color/com_facebook_button_send_background_color_pressed"
            android:text="Back" />
    </LinearLayout>

</android.support.constraint.ConstraintLayout>
